library(tidyverse)
# Read the TSV file
df <- read_tsv("all_info.tsv")
df <- df %>%
mutate(length = nchar(ProteinLength))
ggplot(df, aes(x = ProteinLength, fill = label)) +
geom_density(alpha = 0.5) +
labs(x = "Protein length", y = "Density", fill = "Label") +
theme_minimal() +
facet_wrap(~Set)
scale_x_continuous(limits = c(0, 10000))
