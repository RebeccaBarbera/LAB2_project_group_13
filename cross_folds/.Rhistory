library(tidyverse)
#read the TSV file
df <- read_tsv("all_info.tsv")
df<- df %>%
filter(label == "positive", !is.na(SPPosition))
# calculate median SPPosition for each Set
medians <- df %>%
group_by(Set) %>%
summarize(median_SP = median(SPPosition))
