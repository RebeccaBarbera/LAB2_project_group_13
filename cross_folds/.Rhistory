library(tidyverse)
library(ggpubr)
# read the TSV file
df <- read_tsv("all_info.tsv")
# filter only positive sequences with SPPosition
df <- df %>%
filter(label == "positive", !is.na(SPPosition))
# calculate median SPPosition for each Set
medians <- df %>%
group_by(Set) %>%
summarize(median_SP = median(SPPosition)) %>%
arrange(Set)
# create the density plot
p <- ggplot(df, aes(x=SPPosition, fill=label)) +
geom_density(alpha=0.5) +
labs(title="Signal Peptide Position Distribution",
x="SP Position", y="Density", fill="Label") +
theme_minimal() +
facet_wrap(~Set) +
scale_x_log10() +
scale_fill_manual(values = c("positive" = "red"))
